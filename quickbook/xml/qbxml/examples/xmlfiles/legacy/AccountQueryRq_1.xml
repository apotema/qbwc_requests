<!-- filter the output so lots of accounts could cause this to be a bit slow.   -->
<!-- ========================================================================== -->
<!-- Language: xml                                                            -->
<!-- Structure: QBXML                                                            -->
<!-- QuickBooks: any                                                            -->
<!-- Deployment: any                                                            -->
<!-- ========================================================================== -->
<?qbxml version="1.0"?>
<QBXML>
   <QBXMLMsgsRq onError="stopOnError">
      <AccountQueryRq requestID="1">
      </AccountQueryRq>
   </QBXMLMsgsRq>
</QBXML>

